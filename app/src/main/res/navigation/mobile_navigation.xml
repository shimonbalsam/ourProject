<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_slideshow">


    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.ourproject.views.MainActivity"
        android:label="Add parcel"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.ourproject.views.ParcelsList"
        android:label="Parcel list"
        tools:layout="@layout/parcels_list" />
    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.ourproject.views.ui.slideshow.SlideshowFragment"
        android:label="@string/menu"
        tools:layout="@layout/fragment_slideshow" />



    <fragment
        android:id="@+id/nav_tools"
        android:name="com.example.ourproject.views.ui.tools.ToolsFragment"
        android:label="@string/menu_tools"
        tools:layout="@layout/fragment_tools" />

    <fragment
        android:id="@+id/nav_share"
        android:name="com.example.ourproject.views.ui.share.ShareFragment"
        android:label="@string/menu_share"
        tools:layout="@layout/fragment_share" />

    <fragment
        android:id="@+id/nav_send"
        android:name="com.example.ourproject.views.ui.send.SendFragment"
        android:label="@string/menu_send"
        tools:layout="@layout/fragment_send" />
</navigation>